{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "build:ios:prod": "eas build --platform ios --profile production",
    "build:android:prod": "eas build --platform android --profile production",
    "build:ios:staging": "eas build --platform ios --profile staging",
    "build:android:staging": "eas build --platform android --profile staging",
    "build:staging": "eas build --profile staging",
    "build": "eas build --profile production",
     "publish:staging": "eas update --branch staging --platform ios && eas update --branch staging --platform android",
    "publish:prod": "eas update --branch production --platform ios && eas update --branch production --platform android"
  },
  "dependencies": {
    "@elevenlabs/react-native": "^0.5.10",
    "@expo/vector-icons": "^15.0.3",
    "@lottiefiles/dotlottie-react": "^0.13.5",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.33",
    "expo-av": "~16.0.8",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.10",
    "lottie-react-native": "^7.3.5",
    "lucide-react-native": "^0.563.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
